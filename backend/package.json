{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "server.js",
  "imports": {
    "#routes/*": "./routes/*",
    "#services/*": "./services/*",
    "#servicesHelpers/*": "./services/helpers/*",
    "#db/*": "./db/*",
    "#utils/*": "./utils/*",
    "#controllers/*": "./controllers/*",
    "#middlewares/*": "./middlewares/*"
  },
  "scripts": {
    "test": "pnpm run db:reset:test && dotenv -e .env.testing -- jest",
    "start:dev": "dotenv -e .env.development -- nodemon server.js",
    "start:test": "cross-env NODE_ENV=testing dotenv -e .env.testing -- node server.js",
    "start:prod": "node server.js",
    "db:seed": "node ./db/seed.js",
    "db:seed:dev": "dotenv -e .env.development -- node ./db/seed.js",
    "db:seed:test": "cross-env NODE_ENV=testing dotenv -e .env.testing -- node ./db/seed.js",
    "db:migrate:dev": "dotenv -e .env.development -- knex migrate:latest --env development",
    "db:migrate:test": "dotenv -e .env.testing -- knex migrate:latest --env testing",
    "db:migrate:prod": "knex migrate:latest --env production",
    "db:reset:dev": "pnpm run db:migrate:dev && pnpm run db:seed:dev",
    "db:reset:test": "pnpm run db:migrate:test && pnpm run db:seed:test",
    "check:env:prod": "dotenv -e .env.production -- node check-env.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.16.0",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "16.4.5",
    "express": "^5.1.0",
    "knex": "^3.1.0",
    "mysql2": "^3.14.5",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "babel-jest": "29.7.0",
    "cross-env": "7.0.3",
    "dotenv-cli": "7.4.2",
    "jest": "29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "7.0.0"
  }
}